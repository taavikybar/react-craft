    "normalize.css": "^8.0.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.1"
  },
  "scripts": {
    "prod": "webpack --config webpack.config.prod.js",
    "build": "yarn prod && yarn copy-images && yarn copy-assets",
    "hot": "copyfiles \"index.html\" \"build\" && webpack-dev-server",
    "lint": "yarn eslint && yarn sasslint",
    "lint-fix": "yarn eslint --fix && yarn sasslint",
    "eslint": "eslint ./lib/**/*.{ts,tsx}",
    "sasslint": "sass-lint --config .sasslintrc --no-exit --verbose",
    "server-b": "tsc -p server && babel ./server-build --out-dir ./server-build && yarn server-c",
    "server-c": "copyfiles -u 1 \"server-build/**/*\" \"server-dist\"",
    "server-w": "yarn server-b && watch \"server/**/*\" \"yarn server-b\"",
    "server": "nodemon ./server-dist",
    "server-prod": "http-server build",
    "clean-server": "rm -rf server-dist",
    "clean-fe": "rm -rf dist build",
    "clean": "rm -rf server-dist server-build && yarn clean-fe",
    "convert-md": "sh scripts/convert-md.sh",
    "watch-md": "yarn convert-md && watch \"lib/md/**/*\" \"yarn convert-md\"",
    "copy-images": "copyfiles -u 1 \"lib/img/**/*\" \"build\"",
    "watch-images": "yarn copy-images && watch \"lib/img/**/*\" \"yarn copy-images\"",
    "copy-assets": "copyfiles \"sitemap.xml\" \"build\" && copyfiles \"robots.txt\" \"build\" && copyfiles \"_redirects\" \"build\"",
    "dev": "sh scripts/dev.sh"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint-fix"
    }
  },
  "resolutions": {
    "lodash": "4.17.15",
    "minimatch": "3.0.4"
  }
}
